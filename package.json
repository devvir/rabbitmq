{
  "name": "@devvir/rabbitmq",
  "version": "1.0.0",
  "description": "Generic RabbitMQ wrapper for Node.js applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "private": false,
  "keywords": [
    "rabbitmq",
    "amqp",
    "messaging",
    "queue",
    "broker"
  ],
  "author": "Diego Indart <db.indart.dev@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "amqplib": "^0.10.9",
    "pino": "^10.3.1"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.8",
    "@types/node": "^25.3.0",
    "@vitest/coverage-v8": "^4.0.18",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  }
